<main role="main" class="container">


    <h1>EVENTS</h1>

    <table class="table table-sm">
    <thead>
        <tr>
        <th scope="col">Description</th>
        <th scope="col">location</th>
        <th scope="col">Date and Time</th>
        <th scope="col"></th>

        </tr>
    </thead>
    <tbody>
        <% @events.each do |each| %>
        <tr>
        <td><%= each.description %></td>
        <td><%= each.location %></td>
        <td><%= each.time %></td>
        </tr>
        <% end %>
    </tbody>
    </table>
    

</main><!-- /.container -->